<div class="page-header">
  <h3>Materium #<%= @materium.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Description</dt>
      <dd><%= @materium.description %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/materia" class="btn btn-primary">
