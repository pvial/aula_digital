<div class="page-header">
  <h3>Puntaje #<%= @puntaje.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Nivel</dt>
      <dd>
        <% if @puntaje.nivel.present? %>
          <a href="/nivels/<%= @puntaje.nivel_id %>">
            <%= @puntaje.nivel.user_id %>
          </a>
        <% end %>
      </dd>

      <dt>Categoria</dt>
      <dd>
        <% if @puntaje.materia.present? %>
          <a href="/categoria/<%= @puntaje.categoria_id %>">
            <%= @puntaje.materia.descripcion %>
          </a>
        <% end %>
      </dd>

      <dt>Score</dt>
      <dd><%= @puntaje.score %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/puntajes" class="btn btn-primary">
        Back
      </a>
      <a href="/puntajes/<%= @puntaje.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_puntaje/<%= @puntaje.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
