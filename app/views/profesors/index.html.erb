<div class="page-header">
  <h1>
    Profesors
    <a href="/profesors/new" class="btn btn-lg btn-success">New Profesor</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Email</th>
        <th>Password</th>
        <th>Actions</th>
      </tr>

      <% @profesors.each do |profesor| %>
      <tr>
        <td><%= profesor.email %></td>
        <td><%= profesor.password %></td>
        <td>
          <a href="/profesors/<%= profesor.id %>" class="btn btn-primary">Show</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @profesors, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
